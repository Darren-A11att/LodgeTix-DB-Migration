{
  "name": "lodgetix---reconcile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npx tsx scripts/dev-with-sync.ts",
    "dev:lodgetix": "npx tsx scripts/dev-with-sync-lodgetix.ts",
    "dev:nosync": "npx tsx scripts/dev-nosync.ts",
    "api": "node medusajs/api/server.js",
    "build": "cd mongodb-explorer && npm run build",
    "start": "cd mongodb-explorer && npm run start",
    "sync": "npx tsx scripts/sync-all-data.ts",
    "sync:quick": "npx tsx scripts/sync-all-data.ts --days 7",
    "sync:payments": "npx tsx scripts/sync-all-data.ts --skip-supabase",
    "sync:registrations": "npx tsx scripts/sync-all-data.ts --skip-square --skip-stripe --skip-stripe-connect",
    "sync:square": "npx tsx scripts/sync-all-square-payments.ts",
    "sync:stripe": "npx tsx scripts/sync-all-stripe-payments-multi-account.ts",
    "sync:stripe-connect": "npx tsx scripts/sync-stripe-connect-payments-multi-account.ts",
    "test:stripe": "npx tsx scripts/test-stripe-connection-multi-account.ts",
    "view:stripe": "npx tsx scripts/view-stripe-payments.ts",
    "migrate:stripe": "npx tsx scripts/migrate-stripe-payments-to-main.ts",
    "analyze:schemas": "npx tsx scripts/analyze-payment-schemas.ts",
    "migrate:unified": "npx tsx scripts/migrate-to-unified-payments.ts",
    "migrate:unified:v2": "npx tsx scripts/migrate-to-unified-payments-v2.ts",
    "view:unified": "npx tsx scripts/view-unified-payments.ts",
    "analyze:payments": "npx tsx scripts/analyze-payment-fields-comprehensive.ts",
    "flatten:payments": "npx tsx scripts/flatten-unified-payments.ts",
    "analyze:flattened": "npx tsx scripts/analyze-flattened-payments-similarity.ts",
    "optimize:payments": "npx tsx scripts/create-optimized-payment-schema.ts",
    "flatten:fully": "npx tsx scripts/flatten-payments-fully.ts",
    "analyze:fully-flattened": "npx tsx scripts/analyze-fully-flattened-similarity.ts",
    "create:final-schema": "npx tsx scripts/create-final-unified-schema.ts",
    "analyze:population": "npx tsx scripts/generate-field-population-analysis.ts",
    "analyze:mapping": "npx tsx scripts/generate-field-mapping-analysis.ts",
    "mongodb-explorer": "cd mongodb-explorer && npm run dev",
    "migration-viewer": "cd migration-viewer && npm run dev",
    "invoices:batch": "cd mongodb-explorer && npx tsx src/scripts/generate-invoices-batch.ts",
    "dev:all": "concurrently \"npm run mongodb-explorer\" \"npm run migration-viewer\"",
    "typecheck": "npm run typecheck:root && cd mongodb-explorer && npm run typecheck && cd ../migration-viewer && npm run typecheck",
    "typecheck:root": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "mongodb": "^6.18.0",
    "square": "^43.0.1",
    "stripe": "^18.4.0",
    "uuid": "^11.1.0"
  }
}
