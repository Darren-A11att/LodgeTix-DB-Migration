  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "mongodb": "^6.18.0",
    "square": "^43.0.1"
  }
}
