{
  "name": "lodgetix---reconcile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node scripts/dev-with-sync.js",
    "dev:no-sync": "node scripts/start-dev.js",
    "dev:force-sync": "node scripts/dev-with-sync.js --force-sync",
    "sync": "node scripts/sync-all-data.js",
    "sync:quick": "node scripts/sync-all-data.js --days 7",
    "sync:payments": "node scripts/sync-all-data.js --skip-supabase",
    "sync:registrations": "node scripts/sync-all-data.js --skip-square",
    "dev:concurrent": "concurrently \"npm run server\" \"npm run mongodb-explorer\"",
    "start": "node dist/index.js",
    "migrate": "ts-node src/migrate.ts",
    "import-payments": "ts-node src/import-payments.ts",
    "server": "ts-node --project tsconfig.server.json src/server.ts",
    "server:dev": "nodemon --exec ts-node --project tsconfig.server.json src/server.ts --watch src",
    "mongodb-explorer": "cd mongodb-explorer && npm run dev",
    "migration-viewer": "cd migration-viewer && npm run dev",
    "start:all": "node scripts/start-dev.js",
    "dev:all": "concurrently \"npm run server:dev\" \"npm run mongodb-explorer\" \"npm run migration-viewer\"",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-fix-single": "ts-node --project tsconfig.scripts.json src/scripts/test-single-registration-fix.ts",
    "test-random": "ts-node --project tsconfig.scripts.json src/scripts/test-random-registrations.ts",
    "compare-registration": "ts-node --project tsconfig.scripts.json src/scripts/compare-registration-data.ts",
    "fix-ticket-owners": "ts-node --project tsconfig.scripts.json src/scripts/fix-ticket-owner-ids.ts",
    "verify-ticket-fix": "ts-node --project tsconfig.scripts.json src/scripts/verify-ticket-owner-fix.ts",
    "import-with-ownership": "ts-node --project tsconfig.scripts.json src/scripts/import-registrations-with-correct-ownership.ts",
    "update-ticket-structure": "ts-node --project tsconfig.scripts.json src/scripts/update-ticket-structure-preserve-attendee.ts",
    "analyze-quantity-issue": "ts-node --project tsconfig.scripts.json src/scripts/analyze-ticket-quantity-issue.ts",
    "test-quantity-fix": "ts-node --project tsconfig.scripts.json src/scripts/test-ticket-quantity-fix.ts",
    "fix-ticket-quantities": "ts-node --project tsconfig.scripts.json src/scripts/fix-ticket-quantity-expansion.ts",
    "fix-ticket-quantities-v2": "ts-node --project tsconfig.scripts.json src/scripts/fix-ticket-quantity-expansion-v2.ts",
    "analyze-no-tickets": "ts-node --project tsconfig.scripts.json src/scripts/analyze-registrations-without-tickets.ts",
    "find-empty-registrations": "ts-node --project tsconfig.scripts.json src/scripts/find-truly-empty-registrations.ts",
    "extract-registrations": "ts-node --project tsconfig.scripts.json scripts/extract-all-registrations.ts",
    "analyze-schemas": "ts-node --project tsconfig.scripts.json scripts/analyze-registration-schemas.ts",
    "generate-schema-report": "ts-node --project tsconfig.scripts.json scripts/generate-schema-report.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.0.4",
    "@types/react": "19.1.8",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/pdfkit": "^0.13.4",
    "@types/pg": "^8.15.4",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "jspdf": "^3.0.1",
    "mongodb": "^6.17.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.16.2",
    "square": "^43.0.0",
    "uuid": "^11.1.0"
  }
}
